         xmlns:gnome="http://api.gnome.org/doap-extensions#"
         xmlns="http://usefulinc.com/ns/doap#">
  <name xml:lang="en">Tepl</name>
  <shortdesc xml:lang="en">Text editor product line</shortdesc>
  <description xml:lang="en">Tepl is a library that eases the
development of GtkSourceView-based text editors and IDEs. Tepl
is the acronym for “Text editor product line”.</description>

